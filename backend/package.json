{
  "name": "@imposter/backend",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "bun run src/server.ts",
    "start:new": "bun run src/server-new.ts",
    "dev": "bun run --watch src/server-new.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "build": "bun build src/server-new.ts --outdir dist --target bun",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "uuidv4": "^6.2.13",
    "@imposter/shared": "link:../shared"
  }
}
